{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "container:start": {
            "cache": false
        },
        "container:stop": {
            "cache": false
        },
        "build": {
            "outputs": ["dist/**"]
        },
        "dev": {
            "cache": false
        },
        "lint": {
            "outputs": []
        },
        "test": {
            "dependsOn": ["build"]
        }
    }
}
