{
    "name": "project-next-gen",
    "version": "0.0.1",
    "private": true,
    "workspaces": [
        "packages/*",
        "apps/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "format": "prettier --write \"./**/*.{ts,tsx,md}\"",
        "test": "turbo run test",
        "test:api": "yarn workspace api test"
    },
    "devDependencies": {
        "prettier": "2.6.2",
        "turbo": "1.2.14"
    },
    "license": "MIT"
}
