{
    "name": "project-next-gen",
    "version": "0.0.1",
    "private": true,
    "workspaces": [
        "packages/*",
        "apps/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "format": "prettier --write \"./**/*.{ts,tsx,md}\"",
        "test": "turbo run test",
        "test:api": "yarn workspace api test"
    },
    "devDependencies": {
        "@types/node": "16.0.0",
        "prettier": "2.7.1",
        "typescript": "4.7.4",
        "turbo": "1.3.4"
    },
    "license": "MIT",
    "volta": {
        "node": "16.16.0"
    }
}
