{
    "name": "api",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "swc src -d dist",
        "build:dev": "yarn build && NODE_ENV=development node --inspect dist/index.js",
        "clean": "rm -rf dist",
        "check:types": "tsc --noEmit",
        "dev": "nodemon --ext ts --exec \"yarn build:dev\"",
        "lint": "yarn check:types && eslint src/**/*.ts",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "devDependencies": {
        "@swc/cli": "0.1.57",
        "@swc/core": "1.2.194",
        "@types/express": "4.17.13",
        "@types/node": "17.0.35",
        "eslint": "8.16.0",
        "eslint-config-custom": "*",
        "nodemon": "2.0.16",
        "tsconfig": "*",
        "typescript": "4.7.2"
    },
    "dependencies": {
        "express": "4.18.1"
    },
    "license": "MIT"
}
