{
    "name": "api",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "swc src -d dist",
        "build:dev": "yarn build && NODE_ENV=development node --inspect dist/index.js",
        "clean": "rm -rf dist",
        "check:types": "tsc --noEmit",
        "dev": "nodemon --ext ts --exec \"yarn build:dev\"",
        "lint": "yarn check:types && eslint src/**/*.ts",
        "test": "vitest run"
    },
    "devDependencies": {
        "@swc/cli": "0.1.57",
        "@swc/core": "1.2.194",
        "@types/express": "4.17.13",
        "@types/morgan": "1.9.3",
        "@types/node": "17.0.35",
        "@types/supertest": "2.0.12",
        "eslint": "8.16.0",
        "eslint-config-custom": "*",
        "nodemon": "2.0.16",
        "supertest": "6.2.3",
        "tsconfig": "*",
        "typescript": "4.7.2",
        "vitest": "0.12.9"
    },
    "dependencies": {
        "express": "4.18.1",
        "morgan": "1.10.0"
    },
    "license": "MIT"
}
